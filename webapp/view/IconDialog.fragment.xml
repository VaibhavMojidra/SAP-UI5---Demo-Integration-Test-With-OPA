<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
      id="iconDialog"
      title ="Hello User">
		<content>
			<core:Icon
			src="sap-icon://globe"
            size="8rem"
            class="sapUiMediumMargin"/>
		</content>
		<endButton>
			<Button
				text="Cancel"
				icon="sap-icon://sys-cancel"
				press=".closeIconDialog"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>